<h1>Este es el index de estudiantes</h1>

<%@students.each do |student|%>
  <li><%= student.name %></li>
  <li><%= student.last_name %></li>
  <li><%= student.address %></li>
  <li><%= student.neighborhood%></li>
  <li><%= student.email%></li>
  <li><%= student.school_id%></li>
  <li><%= student.course_id%></li>
<% end %>
